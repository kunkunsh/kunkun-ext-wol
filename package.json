{
  "$schema": "https://schema.kunkun.sh",
  "name": "kunkun-ext-wol",
  "version": "0.0.6",
  "license": "MIT",
  "type": "module",
  "kunkun": {
    "name": "Wake On Lan",
    "shortDescription": "Wake On Lan",
    "longDescription": "Wake On Lan",
    "identifier": "kunkun-ext-wol",
    "permissions": [
      {
        "permission": "shell:deno:spawn",
        "allow": [
          {
            "path": "$EXTENSION/deno-src/index.ts",
            "env": "*",
            "ffi": "*",
            "read": "*",
            "write": "*",
            "sys": "*",
            "run": "*",
            "net": "*"
          }
        ]
      },
      "shell:stdin-write",
      "shell:kill"
    ],
    "demoImages": [],
    "icon": {
      "type": "iconify",
      "value": "arcticons:wakeonlan",
      "hexColor": "#ffff00"
    },
    "customUiCmds": [],
    "templateUiCmds": [
      {
        "name": "Add Wake On Lan Host",
        "main": "dist/add.js",
        "cmds": []
      },
      {
        "name": "Wake On Lan",
        "main": "dist/wake.js",
        "cmds": []
      }
    ]
  },
  "scripts": {
    "dev": "bun build.ts dev",
    "build": "bun build.ts"
  },
  "dependencies": {
    "@kksh/api": "0.0.55",
    "i18next": "^23.15.1",
    "valibot": "^1.0.0-beta.14"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "deno-src",
    "README.md",
    ".gitignore"
  ]
}
